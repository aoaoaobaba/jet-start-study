{
  "name": "webix-jet-dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "forwardPorts": [5173],
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-vscode-remote.remote-containers",
        "dbaeumer.vscode-eslint",
        "esbenp.prettier-vscode"
      ],
      "settings": {
        "terminal.integrated.defaultProfile.linux": "bash"
      }
    }
  },
  "postCreateCommand": "npm install",
  "remoteUser": "node"
}
